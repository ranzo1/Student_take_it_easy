<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"

    android:orientation="vertical">

    <android.support.v7.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="400dp"

        android:layout_height="155dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="10dp"
        card_view:cardBackgroundColor="#f9f9f9"
        card_view:cardCornerRadius="5dp"
        card_view:cardElevation="3dp"



        android:id="@+id/cardViewtimeTable"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true">



        <RelativeLayout

            android:id="@+id/layoutColorOut"
            android:background="@color/colorPrimary"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

        </RelativeLayout>




        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:orientation="vertical">




            <RelativeLayout
                android:id="@+id/layoutColorIn"
                android:background="@color/colorPrimary"
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">




                <com.mikhaellopez.circularimageview.CircularImageView
                    android:id="@+id/circularImageView"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/man"
                    app:civ_border_color="#ffffff"
                    app:civ_border_width="4dp"
                    app:civ_shadow="true"
                    app:civ_shadow_color="#020202"
                    app:civ_shadow_radius="10"
                    android:layout_marginTop="13dp"
                    android:layout_marginLeft="11dp"
                    android:layout_marginStart="11dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:id="@+id/profesorsss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Name"
                    android:gravity="center"
                    android:textColor="#fff"
                    android:layout_marginBottom="3dp"

                    android:layout_below="@+id/circularImageView"
                    android:layout_alignLeft="@+id/circularImageView"
                    android:layout_alignStart="@+id/circularImageView"
                    android:layout_alignRight="@+id/circularImageView"
                    android:layout_alignEnd="@+id/circularImageView" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/relativeLayout5">


                <com.sdsmdg.harjot.vectormaster.VectorMasterView
                    android:id="@+id/bookmark"
                    android:layout_width="50dp"
                    android:layout_height="50dp"

                    app:vector_src="@drawable/ic_bookmark" />


            </RelativeLayout>




            <TextView
                android:id="@+id/nameLabel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Subject"


                android:paddingLeft="5dp"
                android:paddingRight="5dp"

                android:textStyle="bold"
                android:textColor="@color/colorPrimary"
                android:textSize="17dp"
                android:layout_below="@+id/relativeLayout5"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_toRightOf="@+id/layoutColorIn"
                android:layout_toEndOf="@+id/layoutColorIn" />




        </RelativeLayout>

    </android.support.v7.widget.CardView>





</RelativeLayout>